{
  "extends": "eslint-config-airbnb",
  "globals": {
    "chrome": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-use-before-define": [2, "nofunc"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "react/jsx-quotes": 0,
    "react/jsx-no-bind": 1,
    "react/jsx-boolean-value": 0,
    "react/no-multi-comp": [1, {"ignoreStateless": true}],
    "new-cap": 0,
    "block-scoped-var": 0,
    "padded-blocks": 0,
    "quotes": [ 1, "single" ],
    "comma-style": [ 2, "last" ],
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "func-names": 0,
    "prefer-const": 0,
    "comma-dangle": [1, "always-multiline"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "dot-notation": 0,
  },
  "plugins": [
    "react"
  ]
}
